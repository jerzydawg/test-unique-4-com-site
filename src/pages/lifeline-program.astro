---
import Layout from '../layouts/Layout.astro';
import Breadcrumbs from '../components/Breadcrumbs.astro';
import AboveFoldCTA from '../components/AboveFoldCTA.astro';
import RelatedContent from '../components/RelatedContent.astro';
import ProgramCities from '../components/ProgramCities.astro';
import { getSiteURL, getSiteName, getDomain, getKeywordId, getDesignDNA } from '../lib/site-config';
import { loadKeywordVariations } from '../lib/variations/shared/keyword-loader';

const SITE_URL = getSiteURL();
const SITE_NAME = getSiteName();
const DOMAIN = getDomain();
const keywordId = getKeywordId(); // Already has fallback in getKeywordId() function
const designDNA = getDesignDNA();

// Load keyword-specific variations with comprehensive error handling
let h1Content = { h1: 'Lifeline Free Government Phone Program' };
let metaContent = { 
  title: 'Lifeline Free Government Phone | Federal Phone & Internet Discount Program 2025',
  description: 'Access the federal Lifeline program for discounted phone and internet service. Save up to $9.25 monthly on communication costs. Check eligibility instantly and apply online with no credit requirements.'
};

try {
  const keywordModule = await loadKeywordVariations(keywordId);
  if (keywordModule && typeof keywordModule.getH1Variation === 'function' && typeof keywordModule.getMetaVariations === 'function') {
    try {
      h1Content = keywordModule.getH1Variation(DOMAIN, 'lifeline');
      metaContent = keywordModule.getMetaVariations(SITE_NAME, DOMAIN, 'lifeline');
      
      // Validate results
      if (!h1Content || !h1Content.h1) {
        h1Content = { h1: 'Lifeline Free Government Phone Program' };
      }
      if (!metaContent || !metaContent.title || !metaContent.description) {
        metaContent = { 
          title: 'Lifeline Free Government Phone | Federal Phone & Internet Discount Program 2025',
          description: 'Access the federal Lifeline program for discounted phone and internet service. Save up to $9.25 monthly on communication costs. Check eligibility instantly and apply online with no credit requirements.'
        };
      }
    } catch (variationError: any) {
      console.error('Error calling variation functions:', variationError?.message || variationError);
    }
  }
} catch (e: any) {
  console.error('Failed to load keyword variations for lifeline page:', e?.message || e);
  // Use fallback values already set above - page will still render
}

const title = metaContent.title;
const description = metaContent.description;
const canonicalURL = `${SITE_URL}/lifeline-program/`;

// Structured data for better SEO - Rewritten for uniqueness
const structuredData = {
  "@context": "https://schema.org",
  "@type": "Article",
  "headline": "Lifeline Free Government Phone Program | Federal Phone & Internet Service 2025",
  "description": "Comprehensive guide to the federal Lifeline program offering discounted phone and internet service to qualifying households. Learn about eligibility requirements, application process, and program benefits.",
  "image": `${SITE_URL}/og-image.jpg`,
  "author": {
    "@type": "Organization",
    "name": DOMAIN
  },
  "publisher": {
    "@type": "Organization",
    "name": DOMAIN,
    "logo": {
      "@type": "ImageObject",
      "url": `${SITE_URL}/logo.png`
    }
  },
  "datePublished": "2025-01-01",
  "dateModified": "2025-01-01",
  "mainEntityOfPage": {
    "@type": "WebPage",
    "@id": `${SITE_URL}/lifeline-program`
  }
};

const faqData = {
  "@context": "https://schema.org",
  "@type": "FAQPage",
  "mainEntity": [
    {
      "@type": "Question",
      "name": "Can I combine Lifeline with other federal assistance programs?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Yes, eligible households can receive both Lifeline and ACP benefits for maximum communication cost savings."
      }
    },
    {
      "@type": "Question",
      "name": "Are there any credit requirements for Lifeline qualification?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "No, the Lifeline program does not require credit checks or credit history for qualification."
      }
    },
    {
      "@type": "Question",
      "name": "What is the typical approval timeline for Lifeline applications?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Most online applications receive instant approval. If documentation is required, processing typically takes 1â€“3 business days."
      }
    }
  ]
};
---

<Layout title={title} description={description} canonicalURL={canonicalURL}>
  <!-- Structured Data -->
  <script type="application/ld+json" set:html={JSON.stringify(structuredData)} />
  <script type="application/ld+json" set:html={JSON.stringify(faqData)} />

  <!-- Breadcrumbs with Schema -->
  <Breadcrumbs items={[
    { label: 'Home', href: '/' },
    { label: 'Programs', href: '/programs' },
    { label: 'Lifeline Program' }
  ]} />

  <!-- Old Breadcrumb (removing) -->
  <nav class="bg-gray-50 py-3" style="display: none;">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <ol class="flex items-center space-x-2 text-sm text-gray-900">
        <li><a href="/" style={`color: inherit;`} onmouseover={`this.style.color='${designDNA.colors.primary}';`} onmouseout="this.style.color='inherit';">Home</a></li>
        <li class="text-gray-900">/</li>
        <li><a href="/programs" style={`color: inherit;`} onmouseover={`this.style.color='${designDNA.colors.primary}';`} onmouseout="this.style.color='inherit';">Programs</a></li>
        <li class="text-gray-900">/</li>
        <li class="text-gray-900 font-medium">Lifeline Program</li>
      </ol>
    </div>
  </nav>

  <main class="max-w-4xl mx-auto px-4 sm:px-6 lg:px-8 py-8">
    <!-- Hero Section -->
    <div class="text-center mb-8 text-gray-900">
      <h1 class="text-gray-900 text-4xl md:text-5xl font-bold mb-6 ">
        <span style={`color: ${designDNA.colors.primary};`}>{h1Content.h1}</span>
      </h1>
      <p class="text-gray-900 text-xl  max-w-3xl mx-auto mb-8 ">
        The <strong>Lifeline Free Government Phone Program</strong> is a federal initiative designed to make phone and internet service accessible for qualifying Americans. 
        Since 1985, Lifeline has helped millions maintain connections to employment, family, healthcare, and emergency services.
      </p>

      <!-- CTA Above Fold -->
      <AboveFoldCTA context="lifeline" />
      
      <!-- Social Proof -->
      <div style={`background: linear-gradient(135deg, #ECFDF5 0%, ${designDNA.colors.primary}15 100%);`} class="rounded-2xl p-6 mb-8 text-gray-900">
        <div class="grid grid-cols-2 md:grid-cols-4 gap-6 text-center">
          <div>
            <div class="text-3xl font-bold mb-2 text-gray-900" style={`color: ${designDNA.colors.primary};`}>$9.25</div>
            <div class="text-sm text-gray-900">Monthly Discount</div>
          </div>
          <div>
            <div class="text-3xl font-bold text-green-600 mb-2 text-gray-900">2.8M+</div>
            <div class="text-sm text-gray-900">People Helped</div>
          </div>
          <div>
            <div class="text-3xl font-bold text-yellow-600 mb-2 text-gray-900">50</div>
            <div class="text-sm text-gray-900">States Covered</div>
          </div>
          <div>
            <div class="text-3xl font-bold text-purple-600 mb-2 text-gray-900">1985</div>
            <div class="text-sm text-gray-900">Program Started</div>
          </div>
        </div>
      </div>
    </div>

    <!-- Quick Eligibility Checker -->
    <section class="mb-16">
      <div style={`background: linear-gradient(135deg, ${designDNA.colors.primary}, ${designDNA.colors.secondary});`} class="rounded-2xl p-6 text-white text-center">
        <h2 class="text-gray-900 text-3xl font-bold mb-4 ">Verify Your Lifeline Qualification</h2>
        <p class="text-gray-900 text-xl mb-8 max-w-2xl mx-auto">
          Check if you qualify for Lifeline Free Government Phone in just 2 minutes. Most Americans are eligible!
        </p>
        <a href="/apply" rel="noopener noreferrer" class="bg-gradient-to-r from-yellow-400 to-yellow-500 hover:from-yellow-500 hover:to-yellow-600 text-gray-900 font-bold py-4 px-8 rounded-xl text-lg shadow-lg transform hover:scale-105 transition-all duration-200 inline-block text-center">
          CHECK ELIGIBILITY NOW
        </a>
        <p class="text-gray-900 text-sm mt-6" style={`color: ${designDNA.colors.textOnPrimary}CC;`}>
          âš¡ Takes 2 minutes â€¢ ðŸ”’ 100% Secure â€¢ ðŸ’° No hidden fees
        </p>
      </div>
    </section>

    <!-- What is Lifeline -->
    <section class="mb-16">
      <h2 class="text-gray-900 text-3xl font-bold mb-8 text-center">
        What is Lifeline Free Government Phone?
      </h2>
      
      <div class="bg-white rounded-2xl p-6 shadow-lg border border-gray-100">
        <div class="grid md:grid-cols-2 gap-6">
          <div>
            <h3 class="text-gray-900 text-2xl font-bold mb-6 ">Free Government Phone Support</h3>
            <p class="text-gray-900 mb-6 ">
              Lifeline Free Government Phone provides a monthly discount of up to <strong>$9.25</strong> on phone or internet service. 
              The program is available in every state, territory, and on Tribal lands. 
              Qualifying households can choose between a free phone plan or a discount on their existing service. 
              You can combine Lifeline with the <a href="/acp-program" class="text-green-600 hover:underline font-semibold">Affordable Connectivity Program (ACP)</a> for even greater savings. 
              Learn more about <a href="/programs" class="text-blue-600 hover:underline">all available government phone programs</a> and find the best option for your needs.
            </p>
            
            <div style={`background: ${designDNA.colors.primary}15;`} class="rounded-xl p-6">
              <h4 class="text-gray-900 font-semibold mb-3 ">Key Benefits:</h4>
              <ul class="space-y-2 text-gray-900">
                <li class="text-gray-900 flex items-start">
                  <svg class="w-5 h-5 mr-3 mt-0.5 flex-shrink-0" style={`color: ${designDNA.colors.primary};`} fill="currentColor" viewBox="0 0 20 20">
                    <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
                  </svg>
                  Up to $9.25/month discount on phone or internet service
                </li>
                <li class="text-gray-900 flex items-start">
                  <svg class="w-5 h-5 mr-3 mt-0.5 flex-shrink-0" style={`color: ${designDNA.colors.primary};`} fill="currentColor" viewBox="0 0 20 20">
                    <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
                  </svg>
                  Available in all 50 states and territories
                </li>
                <li class="text-gray-900 flex items-start">
                  <svg class="w-5 h-5 mr-3 mt-0.5 flex-shrink-0" style={`color: ${designDNA.colors.primary};`} fill="currentColor" viewBox="0 0 20 20">
                    <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
                  </svg>
                  No contracts or credit checks required
                </li>
                <li class="text-gray-900 flex items-start">
                  <svg class="w-5 h-5 mr-3 mt-0.5 flex-shrink-0" style={`color: ${designDNA.colors.primary};`} fill="currentColor" viewBox="0 0 20 20">
                    <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
                  </svg>
                  Can be combined with other government assistance
                </li>
    </ul>
            </div>
          </div>
          
          <div class="bg-green-50 rounded-xl p-6">
            <h4 class="text-gray-900 font-semibold mb-4 ">Program History:</h4>
            <div class="space-y-4">
              <div class="flex items-start">
                <div class="w-8 h-8 bg-green-600 text-white rounded-full flex items-center justify-center text-sm font-bold mr-3 flex-shrink-0">1985</div>
                <div>
                  <h5 class="text-gray-900 font-semibold ">Program Established</h5>
                  <p class="text-gray-900 text-sm ">Lifeline was created to ensure all Americans have access to essential communication services</p>
                </div>
              </div>
              <div class="flex items-start">
                <div class="w-8 h-8 bg-green-600 text-white rounded-full flex items-center justify-center text-sm font-bold mr-3 flex-shrink-0">2005</div>
                <div>
                  <h5 class="text-gray-900 font-semibold ">Internet Service Added</h5>
                  <p class="text-gray-900 text-sm ">Program expanded to include broadband internet service</p>
                </div>
              </div>
              <div class="flex items-start">
                <div class="w-8 h-8 bg-green-600 text-white rounded-full flex items-center justify-center text-sm font-bold mr-3 flex-shrink-0">2025</div>
                <div>
                  <h5 class="text-gray-900 font-semibold ">Current Program</h5>
                  <p class="text-gray-900 text-sm ">Helping millions of Americans stay connected with modern communication services</p>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- Eligibility Section -->
    <section class="mb-16">
      <h2 class="text-gray-900 text-3xl font-bold mb-8 text-center">
        Who Qualifies for Lifeline?
      </h2>
      
      <div class="grid md:grid-cols-2 gap-6">
        <!-- Income-Based Eligibility -->
        <div class="bg-white rounded-2xl p-6 shadow-lg border border-gray-100">
          <h3 class="text-gray-900 text-2xl font-bold mb-6 ">Income-Based Eligibility</h3>
          <p class="text-gray-900 mb-6 ">
            You qualify if your household income is at or below <strong>135% of the Federal Poverty Guidelines</strong>.
          </p>
          
          <div class="bg-gray-50 rounded-xl p-6">
            <h4 class="text-gray-900 text-lg font-semibold mb-4 ">2025 Income Limits</h4>
            <div>
              <table class="w-full text-sm">
                <thead>
                  <tr class="border-b border-gray-200">
                    <th class="text-left py-2 font-semibold text-gray-900">Household Size</th>
                    <th class="text-left py-2 font-semibold text-gray-900">Max Annual Income</th>
                  </tr>
                </thead>
                <tbody class="text-gray-900">
                  <tr class="border-b border-gray-100">
                    <td class="py-2">1 person</td>
                    <td class="py-2">$20,331</td>
                  </tr>
                  <tr class="border-b border-gray-100">
                    <td class="py-2">2 people</td>
                    <td class="py-2">$27,594</td>
                  </tr>
                  <tr class="border-b border-gray-100">
                    <td class="py-2">3 people</td>
                    <td class="py-2">$34,857</td>
                  </tr>
                  <tr class="border-b border-gray-100">
                    <td class="py-2">4 people</td>
                    <td class="py-2">$42,120</td>
                  </tr>
                  <tr>
                    <td class="py-2">Each additional</td>
                    <td class="py-2">+ $7,263</td>
                  </tr>
                </tbody>
              </table>
            </div>
            <p class="text-gray-900 text-xs  mt-3">*Income limits updated for 2025. Check your state for exact numbers.</p>
          </div>
        </div>

        <!-- Program-Based Eligibility -->
        <div class="bg-white rounded-2xl p-6 shadow-lg border border-gray-100">
          <h3 class="text-gray-900 text-2xl font-bold mb-6 ">Program-Based Eligibility</h3>
          <p class="text-gray-900 mb-6 ">
            You automatically qualify if you or someone in your household participates in one of these government assistance programs:
          </p>
          
          <div class="space-y-3">
            <div class="flex items-center p-3 bg-green-50 rounded-lg">
              <svg class="w-5 h-5 text-green-600 mr-3 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
              </svg>
              <span class="text-gray-900 font-medium">SNAP (Food Stamps)</span>
            </div>
            <div class="flex items-center p-3 bg-green-50 rounded-lg">
              <svg class="w-5 h-5 text-green-600 mr-3 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
              </svg>
              <span class="text-gray-900 font-medium">Medicaid</span>
            </div>
            <div class="flex items-center p-3 bg-green-50 rounded-lg">
              <svg class="w-5 h-5 text-green-600 mr-3 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
              </svg>
              <span class="text-gray-900 font-medium">Supplemental Security Income (SSI)</span>
            </div>
            <div class="flex items-center p-3 bg-green-50 rounded-lg">
              <svg class="w-5 h-5 text-green-600 mr-3 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
              </svg>
              <span class="text-gray-900 font-medium">Federal Public Housing Assistance (Section 8)</span>
            </div>
            <div class="flex items-center p-3 bg-green-50 rounded-lg">
              <svg class="w-5 h-5 text-green-600 mr-3 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
              </svg>
              <span class="text-gray-900 font-medium">Veterans Pension or Survivors Benefit</span>
            </div>
            <div class="flex items-center p-3 bg-green-50 rounded-lg">
              <svg class="w-5 h-5 text-green-600 mr-3 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
              </svg>
              <span class="text-gray-900 font-medium">Tribal-specific programs (Tribal TANF, BIA General Assistance)</span>
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- How to Apply -->
    <section class="mb-16">
      <h2 class="text-gray-900 text-3xl font-bold mb-8 text-center">
        How to Apply for Lifeline
      </h2>
      
      <div style={`background: linear-gradient(135deg, ${designDNA.colors.primary}15, ${designDNA.colors.secondary}15);`} class="rounded-2xl p-6">
        <div class="grid md:grid-cols-2 gap-6">
          <div>
            <h3 class="text-gray-900 text-2xl font-bold mb-6 ">Step-by-Step Application Process</h3>
            <ol class="space-y-4">
              <li class="text-gray-900 flex items-start">
                <span class="text-gray-900 flex-shrink-0 w-8 h-8 text-white rounded-full flex items-center justify-center text-sm font-bold mr-4" style={`background: ${designDNA.colors.primary};`}>1</span>
                <div>
                  <h4 class="text-gray-900 font-semibold ">Check Your Eligibility</h4>
                  <p class="text-gray-900">Use our <a href="/eligibility" style={`color: ${designDNA.colors.primary};`} class="underline">eligibility checker</a> to see if you qualify in just 2 minutes</p>
                </div>
              </li>
              <li class="text-gray-900 flex items-start">
                <span class="text-gray-900 flex-shrink-0 w-8 h-8 text-white rounded-full flex items-center justify-center text-sm font-bold mr-4" style={`background: ${designDNA.colors.primary};`}>2</span>
                <div>
                  <h4 class="text-gray-900 font-semibold ">Gather Required Documents</h4>
                  <p class="text-gray-900">Proof of income or program participation, photo ID, and address verification</p>
                </div>
              </li>
              <li class="text-gray-900 flex items-start">
                <span class="text-gray-900 flex-shrink-0 w-8 h-8 text-white rounded-full flex items-center justify-center text-sm font-bold mr-4" style={`background: ${designDNA.colors.primary};`}>3</span>
                <div>
                  <h4 class="text-gray-900 font-semibold ">Choose a Provider</h4>
                  <p class="text-gray-900">Compare <a href="/providers" style={`color: ${designDNA.colors.primary};`} class="underline">providers</a> in your area and select the best option</p>
                </div>
              </li>
              <li class="text-gray-900 flex items-start">
                <span class="text-gray-900 flex-shrink-0 w-8 h-8 text-white rounded-full flex items-center justify-center text-sm font-bold mr-4" style={`background: ${designDNA.colors.primary};`}>4</span>
                <div>
                  <h4 class="text-gray-900 font-semibold ">Apply Online</h4>
                  <p class="text-gray-900">Complete your application online or through your chosen provider</p>
                </div>
              </li>
    </ol>
          </div>
          
          <div class="bg-white rounded-xl p-6">
            <h3 class="text-gray-900 text-xl font-bold mb-4 ">Required Documents</h3>
            <ul class="space-y-3 text-gray-900">
              <li class="text-gray-900 flex items-start">
                <svg class="w-5 h-5 text-green-500 mr-3 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
                </svg>
                Proof of income (pay stub, tax return, Social Security statement)
              </li>
              <li class="text-gray-900 flex items-start">
                <svg class="w-5 h-5 text-green-500 mr-3 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
                </svg>
                Proof of participation in qualifying programs
              </li>
              <li class="text-gray-900 flex items-start">
                <svg class="w-5 h-5 text-green-500 mr-3 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
                </svg>
                Photo ID (driver's license, state ID, passport)
              </li>
